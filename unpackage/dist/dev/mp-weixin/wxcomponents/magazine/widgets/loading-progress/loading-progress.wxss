.progress-container {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  overflow: hidden;
  color: #fff;
}

.loading-text {
  position: relative;
  top: -80rpx;
  font-size: 36rpx;
}

.loading-center-absolute {
  width: 150rpx;
  height: 150rpx;
  position: relative;
  top: -160rpx;
}

.object {
  width: 20rpx;
  height: 20rpx;
  background-color: #fff;
  position: absolute;
  border-radius: 50% 50% 50% 50%;
  animation: animate 0.8s infinite;
}

.object_one {
  top: 19rpx;
  left: 19rpx;
}

.object_two {
  top: 0rpx;
  left: 65rpx;
  animation-delay: 0.1s;
}

.object_three {
  top: 19rpx;
  left: 111rpx;
  animation-delay: 0.2s;
}

.object_four {
  top: 65rpx;
  left: 130rpx;
  animation-delay: 0.3s;
}

.object_five {
  top: 111rpx;
  left: 111rpx;
  animation-delay: 0.4s;
}

.object_six {
  top: 130rpx;
  left: 65rpx;
  animation-delay: 0.5s;
}

.object_seven {
  top: 111rpx;
  left: 19rpx;
  animation-delay: 0.6s;
}

.object_eight {
  top: 65rpx;
  left: 0rpx;
  animation-delay: 0.7s;
}

@keyframes animate {
  50% {
    transform: scale(1.5, 1.5);
  }

  100% {
    transform: scale(1, 1);
  }
}
